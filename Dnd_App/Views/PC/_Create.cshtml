<script>
               $(document).ready(
                    function updatebonusarmor() {
                        $("#armorBonus").focusout(function (event) {

                            var val = $("#armorBonus").val();

                            if (isNaN(val)) {
                                Alertoast('Armor bonus value must be a number', 'Armor bonus value was changed to 0');
                                hd = "0";
                                $("#armorBonus").val("0");
                            }

                            if (val == "") {
                                Alertoast('Armor bonus value can not be empty', 'Armor bonus value was changed to 0');
                                hd = "0";
                                $("#armorBonus").val("0");
                            }


                           $.ajax({
                               type: "POST",
                               cache: false,
                                url: "@Url.Action("_updateBonusArmor")",
                                dataType: "html",
                                contentType: "application/json; charset=utf-8",
                                data: JSON.stringify({ 'bonus': val, 'TempID':@Model.TempID }),
                                success: function (data)
                                {
                                    reloadpcView();
                                },
                                failure: function (errMsg) {
                                    console.log("error size")
                                    alert(errMsg);
                                }
                            });


                        });
                    }
                );


</script>


<!--HP ---------------------------------------------------------------------------------------------------->
    <!--updatehitPoint-->
