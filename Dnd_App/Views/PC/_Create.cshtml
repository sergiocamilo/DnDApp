                            if (!@Model.presetLanguagesSpeak.Any(m => m.LanguageName == lan))
                            {
                                <option value="@((int)lan)">@lan.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@lan.ToString() </option>
                            }
                        }
                    </select>
                    <label>Select multiple language speak</label>
                </div>
                <label>
                    Languages (@Model.race):
                    <br />
                    @for (int i = 0; i < Model.presetLanguagesSpeak.Count; i++)
                    {

                        <text>
                            @Model.presetLanguagesSpeak[i].LanguageName
                        </text>
                        <br />
                    }
                </label>
            </div>

            <div class="row">
                <div class="input-field col s6">
                    <select multiple="multiple" id="languagesUnderstandMultiple">
                        @foreach (Dnd_App.Models.Enum.LanguageName lan in Enum.GetValues(typeof(Dnd_App.Models.Enum.LanguageName)))
                        {
                            if (!@Model.presetLanguagesSpeak.Any(m => m.LanguageName == lan))
                            {
                                <option value="@((int)lan)">@lan.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@lan.ToString() </option>
                            }
                        }
                    </select>
                    <label>Select multiple language understand</label>
                </div>

                @*<div class="input-field col s6">
                        @Html.EditorFor(model => model.telepathy, new { htmlAttributes = new { @type = "text", @id = "telepathy" } })
                        <label for="telepathy">Telepathy</label>
                    </div>*@



            </div>
        </div>
    </li>


    <!--Special Traits -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_spt" onclick="collapseAll();">Special Traits</div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div id="_npcSpecialTraits"></div>
                <center>
                    <a onclick="addSpecialTraits();" class="btn-floating btn-large waves-effect waves-light red"><i class="material-icons">add</i></a>
                </center>
            </div>
        </div>
    </li>

    <!--Actions -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_act" onclick="collapseAll();">Actions</div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div id="_npcActions"></div>
                <center>
                    <a onclick="addAction(1);" class="btn-floating btn-large waves-effect waves-light red"><i class="material-icons">add</i></a>
                </center>
            </div>
        </div>
    </li>

</ul>


<script src="~/Scripts/materialize/initform.js"></script>
<script src="~/Scripts/materialize/loadform.js"></script>


<script>
        function collapseAll() {
            $("#basic_head").removeClass(function () {
                return "active";
            });
            $("#basic_collap").collapsible({ accordion: true });
            $("#basic_collap").collapsible({ accordion: false });
        }
</script>
<script>
            function collapseAll_create() {
                $("#collap_body_acl").removeClass(function () {
                    return "active";
                });
                $("#collap_body_htp").removeClass(function () {
                    return "active";
                });
                $("#collap_body_spd").removeClass(function () {
                    return "active";
                });
                $("#collap_body_abs").removeClass(function () {
                    return "active";
                });
                $("#collap_body_stw").removeClass(function () {
                    return "active";
                });
                $("#collap_body_skl").removeClass(function () {
                    return "active";
                });
                $("#collap_body_vri").removeClass(function () {
                    return "active";
                });
                $("#collap_body_sen").removeClass(function () {
                    return "active";
                });
                $("#collap_body_lan").removeClass(function () {
                    return "active";
                });
                $("#collap_body_spt").removeClass(function () {
                    return "active";
                });
                $("#collap_body_act").removeClass(function () {
                    return "active";
                });
                $("#collap_create").collapsible({ accordion: true });
                $("#collap_create").collapsible({ accordion: false });
            }
</script>



<!--BASIC INFO -------------------------------------------------------------------------------------------->

    <!--updateName-->
    <script>
               $(document).ready(
                    function updateName2() {
                        $("#name").focusout(function (event) {

                            var name = $("#name").val();

                            if (name == "") {
                                alert('Name can not be empty');
                                Materialize.toast('The name was changed to "Name"', 3000);
                                name = "Name";
                                $("#name").val("Name");
                            }

        
                            $.ajax({
                               type: "POST",
                               cache: false,
                                url: "@Url.Action("UpdateName")",
                                dataType: "html",
                                contentType: "application/json; charset=utf-8",
                                data: JSON.stringify({ 'newName': name, 'TempID':@Model.TempID }),
                                success: function (data)
                                {
                                    reloadpcView();
                                },
                                failure: function (errMsg) {
                                    console.log("error size")
                                    alert(errMsg);
                                }
                            });

                        });
                    }
                );
    </script>

