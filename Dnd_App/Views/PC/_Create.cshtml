                    </select>
                    <label>Select multiple senses</label>
                </div>
                <div id="_pcSenses"></div>
                <div class="col s12">
                    <label>
                        Senses (@Model.race):
                        <br />
                        @for (int i = 0; i < Model.presetSenses.Count; i++)
                        {

                            <text>
                                @Model.presetSenses[i].TypeSense: @Model.presetSenses[i].range ft.
                            </text>
                            <br />
                        }
                    </label>
                </div>

            </div>
        </div>
    </li>

    <!--Languages -->
    <li>
