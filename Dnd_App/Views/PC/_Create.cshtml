@model Dnd_App.Models.Characters.PC

<ul id="collap_create" class="collapsible" data-collapsible="accordion" style="margin-top: 0px;">
    <!--Armor Class -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_acl" onclick="collapseAll();">
            ArmorClass
        </div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div class="col s8">
                    <label>
                        Armor Class: @Model.armorClass.Name

                        @if (@Model.armorClass.Shield)
                        {
                            <text>+ Shield</text>
                        }
                    </label>
                </div>
                <div class="input-field col s4">
                    @Html.EditorFor(model => model.armorBonus, new { htmlAttributes = new { @type = "text" } })
                    <label for="name">Bonus</label>
                </div>
                <div class="input-field col s4">
                    
                </div>

            </div>
        </div>
    </li>

    <!--Hitpoint -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_htp" onclick="collapseAll();">
            Hitpoints
        </div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div class="col s8">
                    <label> @Model.level.Value level,(@Model.hitPoint.HitPointsAVG) HitDice @Model.hitPoint.Die@Model.hitPoint.TypeDie </label>
                </div>
                <div class="input-field col s4">
                    @Html.EditorFor(m => m.HP, new { htmlAttributes = new { @type = "text", @id = "hitPoint" } })
                    <label for="hitPoint">HP</label>
                </div>
            </div>

        </div>
    </li>

    <!--Speeds -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_spd" onclick="collapseAll();">
            Speeds
        </div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div class="col s4">
                    <label>
                        Speeds (@Model.race):
                        <br />
                        @for (int i = 0; i < Model.presetSpeeds.Count; i++)
                        {
                            <text>
                                @Model.presetSpeeds[i].TypeSpeed @Model.presetSpeeds[i].Speedft ft.
                            </text>
                            <br />
                        }
                    </label>
                </div>
                <div class="input-field col s8">
                    <select multiple="multiple" id="speedsMultiple">
                        @foreach (Dnd_App.Models.Enum.TypeSpeed speed in Enum.GetValues(typeof(Dnd_App.Models.Enum.TypeSpeed)))
                        {
                            if (!@Model.presetSpeeds.Any(m => m.TypeSpeed == speed))
                            {
                                <option value="@((int)speed)">@speed.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@speed.ToString() </option>
                            }
                        }
                    </select>
                    <label>Select multiple speeds</label>
                </div>
                <div id="_pcSpeeds"></div>
            </div>
        </div>
    </li>


    <!--Ability Scores -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_abs" onclick="collapseAll();">
            Ability Scores
        </div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">

                @for (int i = 0; i < Model.abilitiesScores.Count; i++)
                {
                    <div class="input-field col s4">
                        @Html.HiddenFor(m => m.abilitiesScores[i].Name)
                        @Html.HiddenFor(m => m.abilitiesScores[i].ShortName)
                        @Html.TextBoxFor(m => m.abilitiesScores[i].Value, new { @type = "text", @id = @Model.abilitiesScores[i].ShortName })
                        @Html.HiddenFor(m => m.abilitiesScores[i].ModValue)
                        <label>@Model.abilitiesScores[i].Name</label>
                    </div>
                }
                <div class="col s6">
                    <label>
                        Bonus Abilities (@Model.race):
                        <br />
                        @for (int i = 0; i < Model.abilitiesScores.Count; i++)
                        {
                            if (@Model.abilitiesScores[i].Bonus > 0)
                            {
                                <text>
                                    @Model.abilitiesScores[i].Name +@Model.abilitiesScores[i].Bonus
                                </text>
                                <br />
                            }
                        }
                    </label>
                </div>
            </div>
        </div>
    </li>

    <!--Saving Throws -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_stw" onclick="collapseAll();">
            Saving throws
        </div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                @for (int i = 0; i < Model.abilitiesScores.Count; i++)
                {
                    <div class="input-field col s6">
                        @if (Model.abilitiesScores[i].SavingThrow && Model.presetSavingThrows.Any(m => m.ModName == Model.abilitiesScores[i].ShortName))
                        {
                            <p>
                                <input disabled="disabled" checked="checked" class="checkbox" data-val="true"
                                       id="ST@(Model.abilitiesScores[i].ShortName)" Name="ST@(Model.abilitiesScores[i].ShortName)"
                                       type="checkbox" value="true">
                                <label for="ST@(Model.abilitiesScores[i].ShortName)">@Model.abilitiesScores[i].Name</label>
                            </p>
                        }
                        else if (Model.abilitiesScores[i].SavingThrow && !Model.presetSavingThrows.Any(m => m.ModName == Model.abilitiesScores[i].ShortName))
                        {
                            <p>
                                <input checked="checked" class="checkbox" data-val="true"
                                       id="ST@(Model.abilitiesScores[i].ShortName)" Name="ST@(Model.abilitiesScores[i].ShortName)"
                                       type="checkbox" value="true">
                                <label for="ST@(Model.abilitiesScores[i].ShortName)">@Model.abilitiesScores[i].Name</label>
                            </p>
                        }
                        else
                        {
                            <p>
                                <input class="checkbox" data-val="false"
                                       id="ST@(Model.abilitiesScores[i].ShortName)" Name="ST@(Model.abilitiesScores[i].ShortName)"
                                       type="checkbox" value="false">
                                <label for="ST@(Model.abilitiesScores[i].ShortName)">@Model.abilitiesScores[i].Name</label>
                            </p>

                        }
                    </div>

                }
            </div>
            <div class="row">
                <div class="col s12">
                    <label>
                        Saving Throws (@Model._class):
                        <br />
                        @for (int i = 0; i < Model.presetSavingThrows.Count; i++)
                        {
                            <text>
                                @Model.presetSavingThrows[i].ModName

                            </text>
                            <br />
                        }
                    </label>
                </div>

            </div>
        </div>
    </li>

    <!--Skills -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_skl" onclick="collapseAll();">Skills</div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div class="input-field col s12">
                    @{ var skills = new Dnd_App.Models.Characters.Skill().ListViewSkills(); }
                    <select multiple="multiple" id="skillsMultiple">
                        @for (int i = 0; i < skills.Count; i++)
                        {
                            if (!@Model.presetSkills.Any(m => m.SkillName == (Dnd_App.Models.Enum.SkillName)i))
                            {
                                <option value="@(i)">@skills[i].ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@skills[i].ToString() </option>
                            }


                        }
                    </select>
                    <label>Select multiple skills</label>
                </div>
                <div id="_pcSkills2"></div>
                <div id="_pcSkills"></div>

                <div class="col s12">
                    <label>
                        Skills (@Model.race):
                        <br />
                        @for (int i = 0; i < Model.presetSkills.Count; i++)
                        {

                            <text>
                                @Model.presetSkills[i].SkillName
                            </text>
                            <br />
                        }
                    </label>
                </div>
            </div>
        </div>
    </li>

    <!--Conditions  (Vulnerabilities, resistances, immunities) -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_vri" onclick="collapseAll();">
            Conditions
        </div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div class="input-field col s6">
                    <select multiple="multiple" id="vulnerabilitiesMultiple">
                        @foreach (Dnd_App.Models.Enum.TypeDamage vul in Enum.GetValues(typeof(Dnd_App.Models.Enum.TypeDamage)))
                        {
                            if (!@Model.presetVulnerabilities.Any(m => m.TypeDamage == vul))
                            {
                                <option value="@((int)vul)">@vul.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@vul.ToString() </option>
                            }

                        }
                    </select>
                    <label>Select multiple damage vulnerabilities</label>
                </div>
                <div class="input-field col s6">
                    <select multiple="multiple" id="resistancesMultiple">
                        @foreach (Dnd_App.Models.Enum.TypeDamage res in Enum.GetValues(typeof(Dnd_App.Models.Enum.TypeDamage)))
                        {
                            if (!@Model.presetResistances.Any(m => m.TypeDamage == res))
                            {
                                <option value="@((int)res)">@res.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@res.ToString() </option>
                            }
                        }
                    </select>
                    <label>Select multiple damage resistances</label>
                </div>
                <div class="input-field col s6">
                    <select multiple="multiple" id="immunitiesDmgMultiple">
                        @foreach (Dnd_App.Models.Enum.TypeDamage imm in Enum.GetValues(typeof(Dnd_App.Models.Enum.TypeDamage)))
                        {
                            if (!@Model.presetImmunitiesDamage.Any(m => m.TypeDamage == imm))
                            {
                                <option value="@((int)imm)">@imm.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@imm.ToString() </option>
                            }

                        }
                    </select>
                    <label>Select multiple damage immunities</label>
                </div>
                <div class="input-field col s6">
                    <select multiple="multiple" id="immunitiesCndMultiple">
                        @foreach (Dnd_App.Models.Enum.TypeCondition imm in Enum.GetValues(typeof(Dnd_App.Models.Enum.TypeCondition)))
                        {
                            if (!@Model.presetImmunitiesCondition.Any(m => m.TypeCondition == imm))
                            {
                                <option value="@((int)imm)">@imm.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@imm.ToString() </option>
                            }

                        }
                    </select>
                    <label>Select multiple condition immunities</label>
                </div>
                <div class="col s12">
                    <label>
                        Conditions (@Model.race):
                        <br />
                        @for (int i = 0; i < Model.presetImmunitiesCondition.Count; i++)
                        {
                            <text>
                                Condition immunity: @Model.presetImmunitiesCondition[i].TypeCondition
                            </text>
                            <br />
                        }
                        @for (int i = 0; i < Model.presetImmunitiesDamage.Count; i++)
                        {
                            <text>
                                Damage immunity : @Model.presetImmunitiesDamage[i].TypeDamage
                            </text>
                            <br />
                        }
                        @for (int i = 0; i < Model.presetResistances.Count; i++)
                        {
                            <text>
                                Damage resistance: @Model.presetResistances[i].TypeDamage
                            </text>
                            <br />
                        }
                        @for (int i = 0; i < Model.presetVulnerabilities.Count; i++)
                        {
                            <text>
                                Damage vulnerability: @Model.presetVulnerabilities[i].TypeDamage
                            </text>
                            <br />
                        }
                    </label>
                </div>
            </div>
        </div>
    </li>


    <!--Senses -->
    <li>
        <div class="collapsible-header teal darken-3 white-text" id="collap_body_sen" onclick="collapseAll();">Senses</div>
        <div class="collapsible-body grey lighten-5">
            <div class="row">
                <div class="input-field col s12">
                    <select multiple="multiple" id="sensesMultiple">
                        @foreach (Dnd_App.Models.Enum.TypeSense sense in Enum.GetValues(typeof(Dnd_App.Models.Enum.TypeSense)))
                        {
                            if (!@Model.presetSenses.Any(m => m.TypeSense == sense))
                            {
                                <option value="@((int)sense)">@sense.ToString() </option>
                            }
                            else
                            {
                                <option value="" disabled>@sense.ToString() </option>
                            }

