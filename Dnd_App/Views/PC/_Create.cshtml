    <!--recalculateAS-->
    <script>
               function recalculateAS(mod, value) {

                   if (isNaN(value)) {
                       Alertoast('Ability Score: ' + mod + ' value must be a number', 'Ability Score: ' + mod + ' value was changed to 10');
                       value = "10";
                       $("#" + mod).val("10");
                   }

                   if (value == "") {
                       Alertoast('Ability Score: ' + mod + ' value can not be empty', 'Ability Score: ' + mod + ' value was changed to 10');
                       value = "10";
                       $("#" + mod).val("10");
                   }

                   if (value < 1) {
                       Alertoast('Ability Score: ' + mod + ' value can not be < 1', 'Ability Score: ' + mod + ' value was changed to 1');
                       value = "1";
                       $("#" + mod).val("1");
                   }

                   if (value > 30) {
                       Alertoast('Ability Score: ' + mod + ' value can not be > 30', 'Ability Score: ' + mod + ' value was changed to 30');
                       value = "30";
                       $("#" + mod).val("30");
                   }


                   $.ajax({
                                   type: "POST",
                                   cache: false,
                                    url: "@Url.Action("RecalculateAS")",
                                    dataType: "html",
                                    contentType: "application/json; charset=utf-8",
                                    data: JSON.stringify({ '_mod': mod, '_value': value, 'TempID':@Model.TempID}),
                                    success: function (data)
                                    {

                                        reloadpcView();
                                    },
                                    failure: function (errMsg) {
                                        console.log("error ?")
                                        alert(errMsg);
                                    }
                                });

               }
    </script>



